{
  "name": "@tsc-run/adapter-aws",
  "version": "0.4.0-alpha.1",
  "description": "AWS adapter with CDK constructs and Lambda adapters for tsc-run framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsc-run/cli.git",
    "directory": "packages/adapter-aws"
  },
  "exports": {
    ".": "./dist/index.js",
    "./secret-resolver": "./dist/secret-resolver.js"
  },
  "homepage": "https://docs.tsc.run",
  "keywords": [
    "typescript",
    "serverless",
    "aws",
    "lambda",
    "cdk"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --test dist/**/*.test.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.817.0",
    "@tsc-run/core": "workspace:*",
    "@tsc-run/utils": "workspace:*",
    "aws-cdk": "^2.0.0",
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "esbuild": "^0.25.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
