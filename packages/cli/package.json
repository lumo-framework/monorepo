{
  "name": "@tsc-run/cli",
  "version": "0.4.0-alpha.3",
  "description": "Command-line interface for the tsc-run TypeScript serverless framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsc-run/cli.git",
    "directory": "packages/cli"
  },
  "homepage": "https://docs.tsc.run",
  "keywords": [
    "typescript",
    "serverless",
    "aws",
    "lambda",
    "cli"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "tsc-run": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --test dist/**/*.test.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tsc-run/adapter-aws": "workspace:*",
    "@tsc-run/core": "workspace:*",
    "@tsc-run/utils": "workspace:*",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.0",
    "cli-table3": "^0.6.5",
    "esbuild": "^0.25.4",
    "express": "^4.21.0",
    "glob": "^11.0.2",
    "ts-morph": "^26.0.0",
    "yargs": "^17.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.4",
    "@types/express": "^4.17.0",
    "@types/node": "^22.15.21",
    "@types/yargs": "^17.0.0",
    "typescript": "^5.0.0"
  }
}
