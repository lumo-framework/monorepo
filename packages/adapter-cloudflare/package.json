{
  "name": "@tsc-run/adapter-cloudflare",
  "version": "0.7.0-next.3",
  "description": "Cloudflare Workers runtime adapter for tsc-run framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsc-run/cli.git",
    "directory": "packages/adapter-cloudflare"
  },
  "exports": {
    ".": "./dist/index.js",
    "./secret-resolver": "./dist/secret-resolver.js",
    "./event-dispatcher": "./dist/event-dispatcher.js"
  },
  "homepage": "https://docs.tsc.run",
  "keywords": [
    "typescript",
    "serverless",
    "cloudflare",
    "workers",
    "runtime"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --test dist/**/*.test.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tsc-run/core": "workspace:*",
    "@tsc-run/utils": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
